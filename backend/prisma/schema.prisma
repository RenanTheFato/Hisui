generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id String @id @default(uuid())
  email String @unique
  password_hash String
  username String

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
  @@map("users")
}